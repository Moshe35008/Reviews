@model Review

<ul>
    <li>Id:@Model.Id</li>
    <li>Name:@Model.Name</li>
    <li>Rank:@Model.Rank</li>
    <li>Description:@Model.Description</li>
</ul>
<form asp-action="Delete">
    <div>
        <label>Are you sure you want to delete this review?</label>
        <input type="submit" value="Delete" />
    </div>
    


</form>